{
  "name": "uniplot",
  "productName": "uniplot",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.mjs",
  "scripts": {
    "start": "electron-forge start",
    "dev:renderer": "vite --config vite.renderer.config.mts",
    "build:electron": "tsc -p tsconfig.node.json --noEmit && vite build --config vite.main.config.mts && vite build --config vite.preload.config.mts",
    "pretest:e2e": "npm run build:electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint . && npm run format:check",
    "lint:fix": "eslint . --fix && npm run format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:unit": "vitest --config vitest.unit.config.ts",
    "test:unit:watch": "vitest --config vitest.unit.config.ts --watch",
    "test:components": "vitest --config vitest.components.config.ts",
    "test:components:watch": "vitest --config vitest.components.config.ts --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "keywords": [],
  "author": {
    "name": "nabby27",
    "email": "26057762+nabby27@users.noreply.github.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "7.9.0",
    "@electron-forge/maker-deb": "7.9.0",
    "@electron-forge/maker-rpm": "7.9.0",
    "@electron-forge/maker-squirrel": "7.9.0",
    "@electron-forge/maker-zip": "7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "7.9.0",
    "@electron-forge/plugin-fuses": "7.9.0",
    "@electron-forge/plugin-vite": "7.9.0",
    "@electron/fuses": "1.8.0",
    "@playwright/test": "1.55.1",
    "@tailwindcss/vite": "4.1.13",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/electron-squirrel-startup": "1.0.2",
    "@types/node": "24.5.2",
    "@types/react": "19.1.14",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "electron": "38.1.2",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.32.0",
    "globals": "15.14.0",
    "jsdom": "27.0.0",
    "lucide-react": "0.544.0",
    "prettier": "3.6.2",
    "rollup": "4.52.3",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.13",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vite": "7.1.7",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.2.3",
    "echarts": "^6.0.0",
    "echarts-for-react": "3.0.2",
    "electron-squirrel-startup": "1.0.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.9.3"
  }
}
