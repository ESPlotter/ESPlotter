{
  "name": "uniplot",
  "productName": "uniplot",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.mjs",
  "scripts": {
    "start": "electron-forge start",
    "dev:renderer": "vite --config vite.renderer.config.mts",
    "build:electron": "tsc -p tsconfig.node.json --noEmit && vite build --config vite.main.config.mts && vite build --config vite.preload.config.mts",
    "pretest:e2e": "npm run build:electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint . && npm run format:check",
    "lint:fix": "eslint . --fix && npm run format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:unit": "vitest --config vitest.unit.config.ts",
    "test:unit:watch": "vitest --config vitest.unit.config.ts --watch",
    "test:components": "vitest --config vitest.components.config.ts",
    "test:components:watch": "vitest --config vitest.components.config.ts --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "keywords": [],
  "author": {
    "name": "nabby27",
    "email": "26057762+nabby27@users.noreply.github.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "7.9.0",
    "@electron-forge/maker-deb": "7.9.0",
    "@electron-forge/maker-rpm": "7.9.0",
    "@electron-forge/maker-squirrel": "7.9.0",
    "@electron-forge/maker-zip": "7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "7.9.0",
    "@electron-forge/plugin-fuses": "7.9.0",
    "@electron-forge/plugin-vite": "7.9.0",
    "@electron/fuses": "1.8.0",
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "1.56.0",
    "@tailwindcss/vite": "4.1.14",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/electron-squirrel-startup": "1.0.2",
    "@types/node": "24.7.0",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "5.0.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "electron": "38.2.2",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "globals": "16.4.0",
    "jsdom": "27.0.0",
    "lucide-react": "0.545.0",
    "prettier": "3.6.2",
    "rollup": "4.52.4",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.14",
    "tw-animate-css": "1.4.0",
    "type-fest": "^5.0.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "vite": "7.1.9",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
